	// --------Pop Up Principal da pagina-----------
	$( document ).ready(function() {
		//AbrirPopUpPrincipal();

		//AbrirPopUpPrincipalEspecial(); //criei outra função pois essa tem uma funcionalidade diferente: ela abre e qdo clicar dentro imagem será fechada a popup e abrirá outra popup

		//O site na Netpacs não abre no Internet Explorer
		$("#linkToNetpacs").click(function() {
			var ua = window.navigator.userAgent;
			var msie = ua.indexOf("MSIE ");

			if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) // If Internet Explorer, return version number
			{
				alert('O navegador Internet Explorer foi descontinuado e não pode mais abrir este endereço. Por favor, utilize outro navegador (Google Chrome, Firefox, Microsoft Edge, outros...)');
				return false;
			}			
		})
		
		//Desabilita o click com o botão esquerdo para não clicar no abrir link
		$("#linkToNetpacs").contextmenu(function() {
			var ua = window.navigator.userAgent;
			var msie = ua.indexOf("MSIE ");
			
			if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) // If Internet Explorer, return version number
			{
				alert('Essa função foi desabilitada.');
				return false;
			}						
		})

		$("#imgBannerPrincipal").on('click', function() {
			AbrirPopUpPrincipalEspecial2();
		})
	});

	function AbrirPopUpPrincipal(){
		$('#dialog-message').empty();
		$('#dialog-message').prepend('<img src="img/carnaval_2019.jpg" height="600px" width="auto"/>');
		$("#dialog-message").dialog({
			"width": 635,
			position: 'center',//{ my: 'top', at: 'top' },
			modal: true
		});
	}
	
